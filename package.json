{
  "name": "cook-share",
  "version": "0.1.0",
  "npmConfig": {
    "legacy-peer-deps": true
  },
  "private": true,
  "scripts": {
    "dev": "start http://localhost:3000 && next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build --build-arg DATABASE_URL=$DATABASE_URL -t cook-share .",
    "docker:run": "docker run --env-file .env -p 3000:3000 cook-share",
    "pretty": "npx prettier --write .",
    "db:open": "npx prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:ingredients": "ts-node prisma/seed/ingredients.ts",
    "db:clear": "ts-node prisma/clear.ts",
    "predb:seed": "npm run db:clear"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@faker-js/faker": "^10.2.0",
    "@formatjs/intl-localematcher": "^0.8.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^7.3.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.563.0",
    "negotiator": "^1.0.0",
    "next": "16.1.4",
    "next-auth": "^4.24.13",
    "next-i18next": "^15.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/negotiator": "^0.6.4",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^25.0.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.23",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "postcss": "^8.5.6",
    "prisma": "^7.3.0",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
